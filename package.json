{
  "name": "starter-sequelize",
  "author": "Dino Baggio",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/server.js",
    "dev": "nodemon --exec babel-node ./src/bin/server.js",
    "build": "rm -R ./dist;babel src --out-dir dist",
    "serve": "node ./dist/bin/server.js",
    "development": "sequelize db:migrate --migrations-path=dist/migrations; node ./dist/bin/server.js",
    "migrate": "sequelize db:migrate --migrations-path=dist/migrations",
    "migrate:rollback": "sequelize db:migrate:undo --migrations-path=dist/migrations",
    "seed": "sequelize db:seed:all --seeders-path=dist/seeders",
    "db-fresh": "npm run build;sequelize db:migrate:undo:all --migrations-path=dist/migrations;sequelize db:migrate --migrations-path=dist/migrations;sequelize db:seed:all --seeders-path=dist/seeders",
    "db-seeder-all": "npm run build;sequelize db:migrate --migrations-path=dist/migrations;sequelize db:seed:all --seeders-path=dist/seeders",
    "db-undo-all": "npm run build;sequelize db:migrate:undo:all --migrations-path=dist/migrations"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-validator": "^6.6.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "morgan": "~1.9.1",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.11",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-unique-token": "^1.0.0",
    "sequelize": "^5.22.3",
    "sequelize-paginate": "^1.1.6",
    "sqlite3": "^5.0.0",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/runtime": "^7.11.2",
    "babel-node": "0.0.1-security",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4"
  }
}
